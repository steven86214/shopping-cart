<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>
{{# each products}}
    <img src = {{this.imagePath}} alt = "..." class ="img-responsive">
    <div class = "caption">
        <h3>title:{{this.title}}</h3>
        <p class = "description">des : {{this.description}}</p>
        <div class = "clearfix">
            <div class = "price pull-left">${{this.price}}</div>
            <a href = "/add-to-cart/" class = "btn-sucess pull-right" role = "button"> Add to Shopping Cart</a>
        </div>
    </div>

{{/each}}